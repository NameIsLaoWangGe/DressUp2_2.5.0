1;\r\n                Laya.LocalStorage.setJSON(\"ManStar\", this.str);\r\n                UIMgr.get(\"UIDraw\").onHide();\r\n                UIMgr.show(\"UIDateSucceed\");\r\n            }\r\n            else {\r\n                console.log(\"成功了！！\");\r\n                UIMgr.get(\"UIDraw\").onHide();\r\n                UIMgr.show(\"UIDateSucceed\");\r\n            }\r\n        }\r\n        else {\r\n            console.log(\"失败了！！\");\r\n            UIMgr.show(\"UIDateFail\");\r\n        }\r\n        this.hide();\r\n    }\r\n}\r\n//# sourceMappingURL=UISure.js.map",
